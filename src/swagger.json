  "components": {
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    },
    "schemas": {
      "practice": {
        "type": "object",
        "required": [
          "modality",
          "studentId"
        ],
        "properties": {
          "modality": { "type": "string" },
          "studentId": { "type": "string" }
        }
      },
      "post-language":{
        "type": "object",
        "required": [
          "name",
          "readingLevel",
          "listeningLevel",
          "speakingLevel",
          "writingLevel",
          "studentId"
        ],
        "properties": {
          "name": { "type": "string" },
          "readingLevel": { "type": "string" },
          "listeningLevel": { "type": "string" },
          "speakingLevel": { "type": "string" },
          "writingLevel": { "type": "string" },
          "studentId": { "type": "string" }
        }
      },
      "update-language": {
        "type": "object",
        "required": [
          "studentId"
        ],
        "properties": {
          "name": { "type": "string" },
          "readingLevel": { "type": "string" },
          "listeningLevel": { "type": "string" },
          "speakingLevel": { "type": "string" },
          "writingLevel": { "type": "string" },
          "studentId": { "type": "string" }
        }
      },
      "post-work-experience":{ 
        "type": "object",
        "required": [
          "company",
          "position",
          "functions",
          "start_date",
          "end_date",
          "studentId"
        ],
        "properties": {
          "company": { "type": "string" },
          "position": { "type": "string" },
          "functions": { "type": "string" },
          "start_date": { "type": "string" },
          "end_date": { "type": "string" },
          "studentId": { "type": "string" }
        }
      },
      "update-work-experience":{ 
        "type": "object",
        "required": [
          "studentId"
        ],
        "properties": {
          "company": { "type": "string" },
          "position": { "type": "string" },
          "functions": { "type": "string" },
          "start_date": { "type": "string" },
          "end_date": { "type": "string" },
          "studentId": { "type": "string" }
        }
      },
      "upload": {
        "type": "object",
        "required": [
          "file",
          "studentId",
          "cedula",
          "table",
          "typeFile"
        ],
        "properties": {
          "file": { "type": "string", "format": "binary" },
          "studentId": { "type": "string" },
          "cedula": { "type": "string" },
          "table": { "type": "string" },
          "typeFile": { "type": "string" }
        }
      },
      "auth": {
        "type": "object",
        "required": [
          "email",
          "password"
        ],
        "properties": {
          "email": { "type": "string" },
          "password": { "type": "string" }
        }
      },
      "student": {
        "type": "object",
        "required": [
          "cedula",
          "firstName",
          "lastName",
          "middleName",
          "birthDate",
          "placeOfBirth",
          "martialStatus",
          "program",
          "address",
          "phone",
          "eps",
          "email",
          "city",
          "userId"
        ],
        "properties": {
          "cedula": { "type": "string" },
          "firstName": { "type": "string" }, 
          "secondName": { "type": "string" },
          "lastName": { "type": "string" },                
          "middleName": { "type": "string" },
          "birthDate": { "type": "string" },
          "placeOfBirth": { "type": "string" },
          "martialStatus": { "type": "string" },
          "program": { "type": "string" },
          "address": { "type": "string" },
          "phone": { "type": "string" },
          "eps": { "type": "string" },
          "email": { "type": "string" },
          "city": { "type": "string" },
          "userId": { "type": "string" }
        }
      }
    }
    
  }
}